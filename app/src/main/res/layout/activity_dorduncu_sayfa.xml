        android:text="Krema"
        android:textColor="@color/white"
        android:textColorHighlight="#F6F6F6"
        android:textColorLink="#F4F4F4"
        android:thumbTint="#FFFFFF"
        android:trackTint="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch4" />

    <Switch
        android:id="@+id/switch10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:backgroundTint="@color/white"
        android:text="Kıtır"
        android:textColor="@color/white"
        android:textColorHighlight="#F6F6F6"
        android:textColorLink="#F4F4F4"
        android:thumbTint="#FFFFFF"
        android:trackTint="#FFFCFC"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/switch7" />

    <Switch
        android:id="@+id/switch3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="5dp"
        android:backgroundTint="@color/white"
        android:text="Dil"
        android:textColor="@color/white"
        android:textColorHighlight="#F6F6F6"
        android:textColorLink="#F4F4F4"
        android:thumbTint="#FFFFFF"
        android:trackTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/switch2"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <Switch
        android:id="@+id/switch6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:backgroundTint="@color/white"
        android:text="Sirke"
        android:textColor="@color/white"
        android:textColorHighlight="#F6F6F6"
        android:textColorLink="#F4F4F4"
        android:thumbTint="#FFFFFF"
        android:trackTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/switch5"
        app:layout_constraintTop_toBottomOf="@+id/switch3" />

    <Switch
        android:id="@+id/switch9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:backgroundTint="@color/white"
        android:text="Kaşar"
        android:textColor="@color/white"
        android:textColorHighlight="#F6F6F6"
        android:textColorLink="#F4F4F4"
        android:thumbTint="#FFFFFF"
        android:trackTint="#FFFFFF"
        app:layout_constraintStart_toEndOf="@+id/switch8"
        app:layout_constraintTop_toBottomOf="@+id/switch6" />

    <Switch
        android:id="@+id/switch12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
